{
  "name": "@ucd-lib/pgdm-ui",
  "version": "0.0.3",
  "description": "GUI for PGDM",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder -ml",
    "dist-windows": "electron-builder -w",
    "watch": "cd app && webpack --config webpack-watch.config.js --watch --display-chunks"
  },
  "postinstall": "electron-builder install-app-deps",
  "repository": "",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@ucd-lib/cork-app-build": "^0.1.5",
    "electron": "^4.0.4",
    "electron-builder": "^20.41.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.3"
  },
  "build": {
    "appId": "edu.ucdavis.library.pgdm",
    "productName": "PostGres Data Management",
    "files": [
      "./app/assets/*",
      "./bundle.js",
      "./index.html",
      "./main.js"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {},
    "linux": {
      "category": "Development"
    }
  },
  "dependencies": {
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-spinner": "^3.0.2",
    "@polymer/polymer": "^3.1.0",
    "@ucd-lib/cork-app-state": "^1.0.0",
    "@ucd-lib/cork-app-utils": "^4.2.3",
    "@ucd-lib/pgdm": "0.0.6",
    "@vaadin/vaadin-upload": "^4.2.1",
    "csv": "^5.1.1",
    "electron-debug": "^3.0.0",
    "lit-element": "^2.1.0",
    "multi-ini": "^2.0.0",
    "pg": "^7.8.1",
    "pg-cursor": "^2.0.0"
  }
}
